<odoo>
    <data>
        <template id="hms_patient_templates">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="patient">
                    <t t-call="web.external_layout">
                            <div class="page">
                                <div class="row">
                                    <div class="col6"></div>
                                </div>
                                <h1 style="color: red;"><span t-out="patient.first_name"/> <span t-out="patient.last_name"/></h1>
                            </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>