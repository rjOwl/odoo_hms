<odoo>
    <data noupdate="1">
        <record id="hms_security_category" model="ir.model.category">
            <field name="name"> HMS</field>
        </record>
        <record id="hms_user_group" model="res_groups">
            <field name="name"> HMS Users</field>
            <field name="category_id" ref="hms_security_category"/>
        </record>

        <record id="hms_manager_group" model="res_groups">
            <field name="name"> HMS Managers</field>
            <field name="category_id" ref="hms_security_category"/>
            <field name="implied_ids" eval="[(4, ref('hms_user_group'))]"/>
        </record>
    </data>
</odoo>


